<h1>Editing entry</h1>

<% form_for [@event, @entry] do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :handle_name %><br />
    <%= f.text_field :handle_name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :present %><br />
    <%= f.text_field :present %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.label :entered_at %><br />
    <%= f.datetime_select :entered_at %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', [@event, @entry] %> |
<%= link_to 'Back', event_entries_path(@event) %>
