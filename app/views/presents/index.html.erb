<h1>Listing presents</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Event</th>
    <th>Sponsor</th>
  </tr>

<% @presents.each do |present| %>
  <tr>
    <td><%=h present.name %></td>
    <td><%=h present.event %></td>
    <td><%=h present.sponsor %></td>
    <td><%= link_to 'Show', present %></td>
    <td><%= link_to 'Edit', edit_present_path(present) %></td>
    <td><%= link_to 'Destroy', present, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New present', new_present_path %>